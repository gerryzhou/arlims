{
    "extends": "../tslint.json",
    "rules": {
       "one-line": false,
       "curly": [true, "ignore-same-line"],
       "member-ordering": false,
        "no-input-rename": false,
        "radix": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
