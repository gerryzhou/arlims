{
   "extends": "../tslint.json",
   "rules": {
      "one-line": false,
      "curly": false,
      "member-ordering": false,
      "no-input-rename": false,
      "radix": false,
      "max-line-length": [true, {"limit": 140, "ignore-pattern": "^import |^export {(.*?)}"}],
      "directive-selector": [
         true,
         "attribute",
         "app",
         "camelCase"
      ],
      "component-selector": [
         true,
         "element",
         "app",
         "kebab-case"
      ]
   }
}
